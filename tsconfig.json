{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "ES6",
    "noImplicitAny": false,
    "outDir": "app",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "sourceMap": true,
  },
  "exclude": ["render/**/*", "!render/preload.js"],
}